Códogo gs

function doGet() {
  return HtmlService.createTemplateFromFile('listaCompras').evaluate();
  }

  function incluir(nomeArquivo) {
  return HtmlService.createHtmlOutputFromFile(nomeArquivo).getContent();  
  }


